<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Inventario" table="inventario">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dataEntrada" type="date" column="data_entrada" nullable="false"/>
    <field name="status" type="string" column="status" length="1" nullable="false"/>
    <field name="valorEntrada" type="decimal" column="valor_entrada" nullable="true"/>
    <many-to-one field="game" target-entity="Game">
      <join-columns>
        <join-column name="game_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="fornecedor" target-entity="Fornecedor">
      <join-columns>
        <join-column name="fornecedor_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
